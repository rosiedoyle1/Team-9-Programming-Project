class Widget
{

  float w = 15;
  float h = 15;
  int x;
  int y;
  int instance;
  color fillColor = color(255);

  Widget(int x, int y, color fillColor, int instance)
  {
    this.x = x;
    this.y = y;
    this.fillColor = fillColor;
    this.instance = instance;
  }


  void draw()
  {
    fill(fillColor);
    rect(x, y, 15, 15);
  }

  int getInstance(int mX, int mY) {
  if (mX > x && mX < x + w && mY > y && mY < y + h) {
    return instance;
  }
  return BUTTON_PRESSED_NULL;
}



  void mouseMoved()
  {
